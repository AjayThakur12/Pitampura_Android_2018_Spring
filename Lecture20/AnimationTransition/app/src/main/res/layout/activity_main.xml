<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/llBackground"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!" />

    <com.codingblocks.animationtransition.MyButton
        android:text="HELLO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <ImageView
        android:src="@drawable/ic_rotate_clockwise_auto"
        android:layout_width="100dp"
        android:layout_height="100dp" />
    <ImageView
        android:id="@+id/ivRotateProgress"
        android:layout_width="100dp"
        android:layout_height="100dp" />
    <com.codingblocks.animationtransition.MyEditText
        android:text="askjdhaskjdh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <com.romainpiel.titanic.library.TitanicTextView
        android:id="@+id/tvTitanic"
        android:text="Loading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@android:color/holo_blue_dark"
        android:textSize="70sp"/>


</LinearLayout>